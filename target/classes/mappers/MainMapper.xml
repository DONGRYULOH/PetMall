<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.dongmall.main.dao.ProductDao">
	
	<!-- 조회수가 가장 높은 상품순으로 상위9개 가져오기  -->
	<select id="getRankNine"  resultType="kr.or.dongmall.main.dto.ProductDto">
			select * from product 
			order by product_hits desc 
			LIMIT 9
	</select> 
</mapper>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	